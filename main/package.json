{
  "private": true,
  "name": "starter",
  "version": "0.0.2",
  "scripts": {
    "start": "node_modules/.bin/serverless offline start --reloadHandler",
    "tail": "NODE_OPTIONS='--dns-result-order=ipv4first' serverless offline start",
    "lint": "eslint -c ./.eslintrc.json . --ext .ts --fix",
    "test": "jest --config=jest.config.ts",
    "predeploy": "./scripts/build-lambda-layers.sh && cd build && serverless deploy --stage staging",
    "deploy": "serverless deploy --stage staging",
    "db:migrate": "node_modules/.bin/knex --knexfile knexfile.ts migrate:latest",
    "db:seed": "npm run db:migrate && TS_NODE_FILES=true node -r tsconfig-paths/register node_modules/.bin/knex --knexfile knexfile.ts seed:run",
    "db:seed:make": "node_modules/.bin/knex --knexfile knexfile.ts seed:make -x ts --",
    "db:rollback": "node_modules/.bin/knex --knexfile knexfile.ts migrate:rollback",
    "db:migrate:new": "node_modules/.bin/knex --knexfile knexfile.ts migrate:make -x ts --",
    "db:migrate:staging": "NODE_ENV=staging node -r ts-node/register database/migration-script.js",
    "db:migrate:production": "NODE_ENV=production node -r ts-node/register database/migration-script.js"
  },
  "dependencies": {
    "@casl/ability": "^6.3.3",
    "@middy/core": "^4.7.0",
    "@middy/http-error-handler": "^4.7.0",
    "@middy/http-json-body-parser": "^4.7.0",
    "@middy/validator": "^4.7.0",
    "cheerio": "^1.0.0",
    "chromedriver": "^133.0.1",
    "http-errors": "^2.0.0",
    "knex": "^2.5.1",
    "objection": "^3.0.1",
    "openai": "^4.85.1",
    "pg": "^8.13.1",
    "puppeteer": "^24.2.1",
    "selenium": "^2.20.0",
    "selenium-webdriver": "^4.28.1",
    "uuid": "^11.0.3",
    "webdriver": "^9.9.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.146",
    "@types/chromedriver": "^81.0.5",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.19.68",
    "@types/selenium-webdriver": "^4.1.28",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "jest": "^29.0.0",
    "prettier": "^3.4.2",
    "serverless": "^4.4.18",
    "serverless-esbuild": "^1.54.6",
    "serverless-offline": "^14.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tunnel-ssh": "^4.1.6",
    "typescript": "^5.7.2"
  }
}
